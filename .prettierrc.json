{
	"parser": "typescript",
	"printWidth": 80,
	"tabWidth": 1,
	"semi": true,
	"bracketSpacing": true,
	"jsxBracketSameLine": false,
	"singleQuote": false,
	"trailingComma": "es5",
	"arrowParens": "always",
	"useTabs": true,
	"overrides": [
		{
			"files": ["*.json"],
			"options": { "parser": "json" }
		}
	]
}
